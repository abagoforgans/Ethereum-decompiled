contract main {


// =======================  Init code  ======================


uint256 stor0; offset 8
uint256 stor0;

function _fallback() payable {
    uint256(stor0.field_0) = Mask(248, 0, stor0.field_8)
    return code.data[63 len 161]
}



// =====================  Runtime code  =====================


uint8 stor0;

function forked() payable {
    return bool(stor0)
}

function _fallback() payable {
  stop
}

function transferringETH(address arg1) payable {
    require stor0
    call arg1 with:
       value msg.value wei
         gas 0 wei
