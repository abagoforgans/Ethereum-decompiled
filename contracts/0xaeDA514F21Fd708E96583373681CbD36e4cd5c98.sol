mapping of uint256 stor0;
array of uint256 stor2;
array of uint256 stor3;
uint8 stor4;
uint256 stor5;
uint256 stor6;

function _fallback() payable {
    bool(stor2.length) = 0
    stor2.length.field_1 = 13
    stor2.length.field_8 = ' Kunkun Token' / 256
    idx = 0
    while stor2.length + 31 / 32 > idx:
        stor2[idx].field_0 = 0
        idx = idx + 1
        continue 
    bool(stor3.length) = 0
    stor3.length.field_1 = 3
    stor3.length.field_8 = 'KKT' / 256
    idx = 0
    while stor3.length + 31 / 32 > idx:
        stor3[idx].field_0 = 0
        idx = idx + 1
        continue 
    stor4 = 18
    stor6 = 100 * 10^6
    require not msg.value
    stor5 = stor6 * 10^stor4
    stor0[address(msg.sender)] = stor5
    return code.data[489 len 2910]
