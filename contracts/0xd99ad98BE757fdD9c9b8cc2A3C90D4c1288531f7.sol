contract main {


// =======================  Init code  ======================


uint256 stor0;
uint256 stor1;

function _fallback() payable {
    require code.data[317 len 32] >= 0
    stor0 = msg.sender or Mask(96, 160, stor0)
    stor1 = code.data[317 len 32]
    return code.data[71 len 246]
}



// =====================  Runtime code  =====================


address owner;
uint256 stor1;
mapping of uint256 stor2;

function owner() payable {
    return owner
}

function _fallback() payable {
  stop
}

function issue(uint256 arg1) payable {
    require msg.sender == owner
    require arg1 + stor1 >= 0
    stor1 += arg1
}

function sub_ded6338b(?) payable {
    require msg.sender == owner
    require stor1 - arg2 >= 0
    require arg2 + stor2[address(arg1)] >= 0
    stor2[address(arg1)] += arg2
    stor1 -= arg2
}



}
