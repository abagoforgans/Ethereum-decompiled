contract main {


// =======================  Init code  ======================


const _fallback = code.data[16 len 222]




// =====================  Runtime code  =====================


uint8 stor0;
uint256 stor0; offset 8
uint256 stor0;

function forked() payable {
    return bool(uint8(stor0.field_0))
}

function _fallback() payable {
  stop
}

function Prism() payable {
    uint256(stor0.field_0) = Mask(248, 0, stor0.field_8)
}

function transferringETH(address arg1) payable {
    require uint8(stor0.field_0)
    call arg1 with:
       value msg.value wei
         gas 0 wei
    require ext_call.success
}

function transferringETC(address arg1) payable {
    require not uint8(stor0.field_0)
    call arg1 with:
       value msg.value wei
         gas 0 wei
    require ext_call.success
}



}
