contract main {


// =======================  Init code  ======================


address stor0;

function _fallback() {
    stor0 = msg.sender
    return code.data[60 len 359]
}



// =====================  Runtime code  =====================


address owner;

function speak() {
    return owner
}

function owner() {
    return owner
}

function _fallback() payable {
    emit Deposit(msg.value, msg.sender);
    call owner with:
       value msg.value wei
         gas 2300 * is_zero(value) wei
    require ext_call.success
}



}
