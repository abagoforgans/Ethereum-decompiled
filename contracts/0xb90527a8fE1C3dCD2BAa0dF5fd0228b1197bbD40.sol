function sub_b781b60a(?) {
    return sha3(block.hash(5000), arg1)
}

function randomGen(uint256 arg1) {
    return (sha3(block.hash(block.number - 1), arg1) % 10000)
}

function rollDice(uint256 arg1) payable {
    require arg1 >= 1
    require arg1 <= 99
    require arg1
    emit 0xd8ae4d0f: block.timestamp
    emit 0xd8ae4d0f: (100 * msg.value / arg1)
    emit 0xd8ae4d0f: ((100 * msg.value / arg1) - msg.value)
    emit 0xd8ae4d0f: msg.value
    emit 0xd8ae4d0f: arg1
    if sha3(block.hash(block.number - 1), block.timestamp) % 10000 >= 100 * arg1:
        emit 0xd8ae4d0f: 0
        return 0
    call msg.sender with:
       value 100 * msg.value / arg1 wei
         gas 2300 * is_zero(value) wei
    emit 0xd8ae4d0f: 1
    return 1
}



