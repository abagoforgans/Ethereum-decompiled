contract main {


// =======================  Init code  ======================


address stor0;
address stor1;
address stor2;
uint256 stor3;

function _fallback() payable {
    stor0 = 0x778e763c4a09c74b2de221b4d3c92d8c7f27a038
    stor1 = 0x778e763c4a09c74b2de221b4d3c92d8c7f27a038
    stor2 = 0x24f929f9ab84f1c540b8ff1f67728246bfec12e1
    stor3 = 3 * 10^18
    require not msg.value
    return code.data[296 len 914]
}



// =====================  Runtime code  =====================


address tokenRewardAddress;
address stor1;
address sub_c7f19241Address;
uint256 shuliang;

function tokenReward() {
    return tokenRewardAddress
}

function shuliang() {
    return shuliang
}

function sub_c7f19241(?) {
    return sub_c7f19241Address
}

function setfanbei(uint256 arg1) {
    require msg.sender == sub_c7f19241Address
    shuliang = arg1
}

function _fallback() payable {
    require msg.value == shuliang
    call stor1 with:
       value msg.value wei
         gas 2300 * is_zero(value) wei
    require ext_call.success
    require ext_code.size(tokenRewardAddress)
    call tokenRewardAddress.jigoutuihuan(address arg1, uint256 arg2) with:
         gas gas_remaining wei
        args msg.sender, 6 * 10^18
    require ext_call.success
}



}
