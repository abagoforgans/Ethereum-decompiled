contract main {


// =======================  Init code  ======================


uint256 stor1;

function _fallback() payable {
    stor1 = msg.sender or Mask(96, 160, stor1)
    return code.data[36 len 326]
}



// =====================  Runtime code  =====================


uint256 playCount;
address owner;
uint256 sub_463d1fca;
uint256 sub_21e1ba4e;

function sub_21e1ba4e(?) payable {
    return sub_21e1ba4e
}

function sub_463d1fca(?) payable {
    return sub_463d1fca
}

function owner() payable {
    return owner
}

function playCount() payable {
    return playCount
}

function terminate() payable {
    require msg.sender == owner
    selfdestruct(owner)
}

function _fallback() payable {
  stop
}

function play() payable {
    playCount++
    emit 0xc3f690f8: address(msg.sender), msg.value, playCount
    sub_463d1fca = msg.value
    if sha3(gas_remaining) % 10 < 3:
        call msg.sender with:
           value 2 * msg.value wei
             gas 0 wei
}



}
