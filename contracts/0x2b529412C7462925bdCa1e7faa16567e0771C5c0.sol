contract main {


// =======================  Init code  ======================


uint256 stor0;

function _fallback() payable {
    emit 0x9a640132: Array(len=42, data='Ab jetzt werden Spenden entgegen', ' genommen!')
    stor0 = msg.sender or Mask(96, 160, stor0)
    return code.data[157 len 719]
}



// =====================  Runtime code  =====================


address stor0;
array of struct stor1;

function kill() payable {
    if stor0 != msg.sender:
    selfdestruct(stor0)
}

function _fallback() payable {
  stop
}

function donate(string arg1) payable {
    call stor0 with:
       value 95 * msg.value / 100 wei
         gas 0 wei
    emit 0x6460c894: Array(len=arg1.length, data=arg1[all]), 95 * msg.value / 100
    stor1.length++
    if not stor1.length <= stor1.length + 1:
        mem[0] = 1
        idx = 2 * stor1.length + 1
        while sha3(1) + (2 * stor1.length) > idx + sha3(mem[0]):
            stor[idx + sha3(mem[0])] = 0
            if 31 < stor[idx + sha3(mem[0])].length:
                mem[0] = idx + sha3(mem[0])
                s = sha3(idx + sha3(mem[0]))
                while sha3(idx + sha3(mem[0])) + (stor[idx + sha3(mem[0])].length + 31 / 32) > s:
                    stor[s] = 0
                    s = s + 1
                    continue 
            stor[idx + sha3(mem[0]) + 1] = 0
            idx = idx + 1
            continue 
    stor1[stor1.length][].field_0 = Array(len=arg1.length, data=arg1[all])
    stor1[stor1.length].field_256 = 95 * msg.value / 100
}



}
